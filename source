local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Poly Hub", "Sentinel")

--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Script Hub")
MainSection:NewButton("Infinite Yield", "Credit to the Creators", function()
	loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
end)
MainSection:NewButton("Remote Spy/Simple Spy", "Credit to the Creators", function()
	loadstring(game:HttpGet("https://github.com/exxtremestuffs/SimpleSpySource/raw/master/SimpleSpy.lua"))()
end)
MainSection:NewButton("Dark Dex V4", "Credit to the Creators", function()
	loadstring(game:HttpGet("https://github.com/exxtremestuffs/SimpleSpySource/raw/master/SimpleSpy.lua"))()
end)
MainSection:NewButton("SL2 Hub", "Credit to the Creators", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/kvinityy/ReaperHubLeak/main/antikurPro"))()
end)

MainSection:NewButton("Homebrew Admin", "Credit to the Creators", function()
	_G.CustomUI = false
	loadstring(game:HttpGet(('https://raw.githubusercontent.com/mgamingpro/HomebrewAdmin/master/Main'),true))()
end)
MainSection:NewButton("Phantom Forces Hub", "Credit to the Creators", function()
	loadstring(game:HttpGet("https://ducksite.xyz/ceri/main.lua"))()
end)
MainSection:NewButton("Cmd X", "Credit to the Creators", function()
	local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
	local Window = Library.CreateLib("Poly Hub", "Sentinel")

	--Main
	local Main = Window:NewTab("Main")
	local MainSection = Main:NewSection("Script Hub")
	MainSection:NewButton("Infinite Yield", "Credit to the Creators", function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
	end)
	MainSection:NewButton("Remote Spy/Simple Spy", "Credit to the Creators", function()
		loadstring(game:HttpGet("https://github.com/exxtremestuffs/SimpleSpySource/raw/master/SimpleSpy.lua"))()
	end)
	MainSection:NewButton("Dark Dex V4", "Credit to the Creators", function()
		loadstring(game:HttpGet("https://github.com/exxtremestuffs/SimpleSpySource/raw/master/SimpleSpy.lua"))()
	end)
	MainSection:NewButton("SL2 Hub", "Credit to the Creators", function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/kvinityy/ReaperHubLeak/main/antikurPro"))()
	end)

	MainSection:NewButton("Homebrew Admin", "Credit to the Creators", function()
		_G.CustomUI = false
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/mgamingpro/HomebrewAdmin/master/Main'),true))()
	end)
	MainSection:NewButton("Phantom Forces Hub", "Credit to the Creators", function()
		loadstring(game:HttpGet("https://ducksite.xyz/ceri/main.lua"))()
	end)
	MainSection:NewButton("Cmd X", "Credit to the Creators", function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source",true))()
	end)
	--Player
	local Player = Window:NewTab("Player")
	local PlayerSection = Player:NewSection("Player")
	PlayerSection:NewSlider("WalkSpeed", "Changes the WalkSpeed", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
	end)
	PlayerSection:NewSlider("Jumppower", "Changes the Jumppower", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
		game.Players.LocalPlayer.Character.Humanoid.Jumppower = s
	end)
	PlayerSection:NewToggle("Click TP", "Gives you Aimbot and ESP", function()
		mouse = game.Players.LocalPlayer:GetMouse()
		tool = Instance.new("Tool")
		tool.RequiresHandle = false
		tool.Name = "OSA Lol"
		tool.Activated:connect(function()
			local pos = mouse.Hit+Vector3.new(0,2.5,0)
			pos = CFrame.new(pos.X,pos.Y,pos.Z)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
		end)
		tool.Parent = game.Players.LocalPlayer.Backpack

	end)
	PlayerSection:NewToggle("God Mode", "Gives you Infinite HP", function()
		local plr = game.Players.LocalPlayer local chr = plr.Character
		chr.Humanoid:Destroy()


		function onKeyPress(actionName, userInputState, inputObject)
			local plr = game.Players.LocalPlayer local chr = plr.Character
			chr.Humanoid:Destroy()
		end


		game.ContextActionService:BindAction("keyPress", onKeyPress, false, Enum.KeyCode.F)

	end)

